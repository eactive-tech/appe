 "font_size": 14,
 "html": "<!DOCTYPE html>\r\n<html>\r\n\r\n<body>\r\n<p style=\"font-size:15px; text-align:center\"><b>{{doc.appe_report}} </b></p>\r\n<hr>\r\n\r\n\r\n\r\n<p style=\"font-size:14px\">Filters</p>\r\n{% set filters = json.loads(doc.filters) %}\r\n{% set all_filters = frappe.get_doc(\"Appe Report\",doc.appe_report)%}\r\n\r\n<table style=\"width: 100%; margin-bottom: 20px;\">\r\n    {% for fltr in all_filters.filters%}\r\n\r\n  <tr style=\"font-size:12px\">\r\n    <td><b>{{fltr.get(\"label\")}}</b> : {{ filters[fltr.get(\"fieldname\")]  }}</td>\r\n  </tr>\r\n  {% endfor %}\r\n</table>\r\n\r\n<p style=\"font-size:14px\">Report Data</p>\r\n{% set results = json.loads(doc.results) %}\r\n<table>\r\n        <thead>\r\n            <tr style=\"font-size:12px\">\r\n                {# English: Render table headers from columns #}\r\n                {# Hindi: Table ke headers columns se banao #}\r\n                {% for col in doc.results.columns %}\r\n                    {% if not col.hidden %}\r\n                        <th>{{ col.label }}</th>\r\n                    {% endif %}\r\n                {% endfor %}\r\n            </tr>\r\n        </thead>\r\n        </table>\r\n\r\n<table class=\"table table-bordered table-condensed\">\r\n  <thead>\r\n    <tr style=\"font-size:12px\">\r\n      {% for col in results.columns %}\r\n        <th>{{ col.label }}</th>\r\n      {% endfor %}\r\n    </tr>\r\n  </thead>\r\n  <tbody>\r\n    {% for row in results.result %}\r\n      <tr style=\"font-size:12px\">\r\n        {% for col in results.columns %}\r\n          {% set value = row.get(col.fieldname) %}\r\n          \r\n          <td style=\"{% if row.indent == 1.0 %}padding-left: 200px;{% endif %}\">\r\n          {{ frappe.format(value, col) if value is not none else \"\" }}\r\n          </td>\r\n        {% endfor %}\r\n      </tr>\r\n    {% endfor %}\r\n  </tbody>\r\n</table>\r\n\r\n</body>\r\n</html>\r\n",
